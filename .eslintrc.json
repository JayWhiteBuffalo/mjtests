{
  "plugins": ["@typescript-eslint"],
  "extends": "next/core-web-vitals",
  "rules": {
    "import/no-anonymous-default-export": "off",
    "no-mixed-operators": "off",
    "no-restricted-globals": "off",
    "react/display-name": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^_",
        "argsIgnorePattern": "^_"
      }
    ]
  }
}
